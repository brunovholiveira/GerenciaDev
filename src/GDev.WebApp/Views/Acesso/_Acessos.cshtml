@model IEnumerable<GDev.WebApp.ViewModels.AcessoViewModel>

@if (Model != null)
{
    <table class="table table-hover">
        <thead class="thead-dark">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Url)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Token)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Modulo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cliente)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DiaCadastro)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DiaAlteracao)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Url)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Token)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Modulo.Descricao)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Cliente.RazaoSocial)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DiaCadastro)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DiaAlteracao)
                    </td>                
                </tr>
            }
        </tbody>
    </table>
}