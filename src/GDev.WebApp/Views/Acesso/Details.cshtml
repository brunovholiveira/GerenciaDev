@model GDev.WebApp.ViewModels.AcessoViewModel

@{
    ViewData["Title"] = "Detalhes do acesso";
}

<h1>@ViewData["Title"]</h1>

<div>    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Url)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Url)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Token)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Token)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Modulo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Modulo.Descricao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cliente)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cliente.RazaoSocial)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DiaCadastro)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DiaCadastro)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DiaAlteracao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DiaAlteracao)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-info" asp-action="Edit" asp-route-id="@Model.Id">Alterar</a>
    <a class="btn btn-secondary" asp-action="Index">Voltar</a>
</div>
