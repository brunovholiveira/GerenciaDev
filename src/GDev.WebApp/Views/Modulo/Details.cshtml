@model GDev.WebApp.ViewModels.ModuloViewModel

@{
    ViewData["Title"] = "Consulta";
}

<h1>@ViewData["Title"]</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Descricao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descricao)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Alterar</a>
    <a class="btn btn-secondary" asp-action="Index">Voltar</a>
</div>
<hr />
<h4>Acessos</h4>
<div>
    <partial name="../Acesso/_Acessos" model="@Model.Acessos" />
</div>
